<div class="row">
	<div class="box col-xs-10 col-xs-offset-1">
		<h1>Edit Your Place</h1>
		<br>
		<%= simple_form_for(@place, wrapper: :vertical_form) do |f| %>
			<%= f.input :name %>
			<%= f.input :address %>
			<%= f.input :description %>
			<br>
			<%= f.submit("Update", class: "btn btn-primary") %>
		<% end %>
	</div>
</div>